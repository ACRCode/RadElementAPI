{
  "ConnectionStrings": {
    "Database": "server=localhost;user id=root;password=root;persistsecurityinfo=True;database=radelement"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile", "Serilog.Sinks.Async" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": { "pathFormat": "Logs/log-{Date}.log" }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "FlightFiles"
    }
  },
  "Environment": {
    "ApplicationURL": "https://localhost:44319/",
    "SwaggerRoutePrefix": "wiki",
    "XMLFilesPath": "wwwroot\\XMLFiles\\"
  },

  "AuthorizationConfig": {
    "Issuer": "assist.acr.org",
    "Audience": "assist.acr.org",
    "ExpiryInMinutes": 30,
    "KeyFilePath": "\\Certificates\\Marval.cer"
  },

  "Integrations": {
    "MarvalService": "https://localhost:44300/marval/api/v1"
  },

