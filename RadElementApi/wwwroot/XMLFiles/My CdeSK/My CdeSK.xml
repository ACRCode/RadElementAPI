<ReportingModule>
  <Metadata>
    <Label>My CdeSK</Label>
    <ID>My_CdeSK</ID>
    <SchemaVersion>1.0</SchemaVersion>
    <ModuleVersion>1.0</ModuleVersion>
    <Info>
      <Diagrams></Diagrams>
      <Description>This module describes the Common Data elements and Macros forMy CdeSK</Description>
      <References>
        <Citation
          Url="http://radelement.org/"> CDE URL </Citation>
        <Citation
          Url="https://docs.google.com/spreadsheets/d/1mXqXAByeur3C5ud-wbLY2vggB29-nSxU5nYpoInGsK4/edit#gid=891834841">
              CDE google sheet </Citation>
      </References>
      <Contact>
        <Name>Reviewer1 test</Name>
        <Email>reviewer1@yopmail.com</Email>
        <Institution>American College of Radiology</Institution>
      </Contact>
    </Info>
  </Metadata>
  <DataElements>
    <IntegerDataElement
      Id="LesionSize"
      IsRequired="true"
      DisplaySequence="1">
      <Label>Lesion Size</Label>
    </IntegerDataElement>
    <ChoiceDataElement
      Id="Choicetype"
      IsRequired="false"
      DisplaySequence="2"
      AllowFreetext="true">
      <Label>Choice type</Label>
      <ChoiceInfo>
        <Choice>
          <Value>1</Value>
          <Label>1</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>2</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
  </DataElements>
  <Rules>
    <DecisionPoint
      Id="macrodp">
      <Label>Macro Banch</Label>
      <Branch>
        <EqualCondition
          DataElementId="conditionConst"
          ComparisonValue="10" />
        <EndPointRef
          EndPointId="macroEndpoint"></EndPointRef>
      </Branch>
    </DecisionPoint>
  </Rules>
  <EndPoints>
    <EndPoint
      Id="macroEndpoint">
      <Label>A</Label>
      <ReportTexts>
        <ReportText
          SectionId="findings">	Sample Report Text</ReportText>
      </ReportTexts>
    </EndPoint>
  </EndPoints>
</ReportingModule>