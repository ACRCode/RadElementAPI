<ReportingModule>
  <Metadata>
    <Label>My LynphNOde CDE</Label>
    <ID>My_LynphNOde_CDE</ID>
    <SchemaVersion>1.0</SchemaVersion>
    <ModuleVersion>1.0</ModuleVersion>
    <Info>
      <Diagrams>
        <Diagram
          DisplaySequence="1"
          IsKeyDiagram="true">
          <Location>keyimage.PNG</Location>
          <Label>keyimage</Label>
        </Diagram>
      </Diagrams>
      <Description>This module describes the Common Data elements and Macros forMy LynphNOde CDE</Description>
      <References>
        <Citation
          Url="http://radelement.org/"> CDE URL </Citation>
        <Citation
          Url="https://docs.google.com/spreadsheets/d/1mXqXAByeur3C5ud-wbLY2vggB29-nSxU5nYpoInGsK4/edit#gid=891834841">
              CDE google sheet </Citation>
      </References>
      <Contact>
        <Name>Sujith Nair</Name>
        <Email>snair@acr.org</Email>
        <Institution>American College of Radiology</Institution>
      </Contact>
    </Info>
  </Metadata>
  <DataElements>
    <ChoiceDataElement
      Id="SInglevsMUltiple"
      IsRequired="true"
      DisplaySequence="1">
      <Label>SIngle vs MUltiple</Label>
      <Hint>number of nodes</Hint>
      <Diagrams>
        <Diagram>
          <Location>keyimage.PNG</Location>
          <Label>keyimage</Label>
        </Diagram>
      </Diagrams>
      <ChoiceInfo>
        <Choice>
          <Value>SIngle</Value>
          <Label>SIngle</Label>
        </Choice>
        <Choice>
          <Value>multiple</Value>
          <Label>multiple</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
    <IntegerDataElement
      Id="diameter"
      IsRequired="true"
      DisplaySequence="2">
      <Label>diameter</Label>
      <Minimum>1</Minimum>
      <Maximum>100</Maximum>
    </IntegerDataElement>
  </DataElements>
  <Rules>
    <DecisionPoint
      Id="macrodp">
      <Label>Macro Banch</Label>
      <Branch>
        <EqualCondition
          DataElementId="conditionConst"
          ComparisonValue="10" />
        <EndPointRef
          EndPointId="macroEndpoint"></EndPointRef>
      </Branch>
    </DecisionPoint>
  </Rules>
  <EndPoints>
    <EndPoint
      Id="macroEndpoint">
      <Label>A</Label>
      <ReportTexts>
        <ReportText
          SectionId="findings">	Sample Report Text</ReportText>
      </ReportTexts>
    </EndPoint>
  </EndPoints>
</ReportingModule>